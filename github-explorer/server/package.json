{
  "name": "github-explorer-pipeline-server",
  "version": "1.0.0",
  "description": "Node.js pipeline server for the GitHub Explorer application",
  "main": "dist/index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src/**/*.ts --exec npx ts-node src/index.ts",
    "build": "rimraf dist && tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:github-api": "npx ts-node src/scripts/test-github-api.ts",
    "test:github-client": "npx ts-node src/scripts/test-github-client.ts",
    "test:github-client-js": "node scripts/test-github-client.cjs",
    "test:github-api-simple": "node scripts/test-github-api-simple.cjs",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [],
  "author": "GitHub Explorer Team",
  "license": "MIT",
  "dependencies": {
    "@octokit/plugin-retry": "^6.0.1",
    "@octokit/plugin-throttling": "^8.1.3",
    "@octokit/rest": "^20.0.2",
    "@supabase/supabase-js": "^2.39.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-request-id": "^3.0.0",
    "helmet": "^8.0.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "typescript": "^5.8.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express-request-id": "^3.0.2",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2"
  }
}

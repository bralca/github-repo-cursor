{
  "name": "github-explorer-pipeline-server",
  "version": "1.0.0",
  "description": "Node.js pipeline server for the GitHub Explorer application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src/**/*.ts --exec npx ts-node src/index.ts",
    "build": "rimraf dist && tsc",
    "build:prod": "npm run lint && npm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "check-types": "tsc --noEmit",
    "check": "npm run lint && npm run check-types",
    "prestart": "npm run build",
    "docker:build": "docker build -t github-explorer-server .",
    "docker:run": "docker run -p 3001:3001 github-explorer-server",
    "docker:dev": "docker-compose up",
    "docker:down": "docker-compose down",
    "prepare-release": "npm run build:prod && npm run test",
    "postinstall": "npm run build"
  },
  "keywords": [],
  "author": "GitHub Explorer Team",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-request-id": "^3.0.0",
    "helmet": "^8.0.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "typescript": "^5.8.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express-request-id": "^3.0.2",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2"
  }
}
